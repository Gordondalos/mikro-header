{
  "name": "micro-header",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxyconfig.js",
    "start-dev-en": "ng serve --proxy-config src/proxyconfig.js --configuration=en-GB",
    "build": "ng build --localize && node rename.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:ts": "prettier --parser typescript --write **/*.{ts,tsx}",
    "format:html": "prettier --parser angular --write **/*.component.html",
    "format": "npm run format:ts && npm run format:html",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:scripts": "ng lint",
    "lint:scripts:fix": "ng lint --fix",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
    "prettier": "lint-staged",
    "run-before-script-extract": "npm install -g ngx-i18nsupport",
    "extract": "ng extract-i18n --output-path src/locale --format=xlf --out-file=messages.xlf && xliffmerge --profile xliffmerge.json ru",
    "deploy": "bash deploy.sh"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "eslint --cache --fix"
    ],
    "**/*.component.html": "prettier --parser angular --write",
    "*.{css,scss}": "stylelint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/elements": "^16.2.9",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@iiko/auth": "^4.0.0",
    "@iiko/basic-layout": "^7.0.0",
    "@iiko/button-spinner": "^2.0.0",
    "@iiko/confirm": "^2.0.0",
    "@iiko/dynamic-panel": "^1.0.0",
    "@iiko/header": "^9.0.0",
    "@iiko/highlight": "^3.0.0",
    "@iiko/language-select": "^4.0.0",
    "@iiko/menu": "^4.0.0",
    "@iiko/news": "^5.0.0",
    "@iiko/page-title": "^5.0.0",
    "@iiko/range-selector": "^8.0.0",
    "@iiko/search": "^4.0.0",
    "@iiko/select-resto": "^4.0.0",
    "@iiko/services": "^8.0.0",
    "@iiko/side-panel": "^2.0.0",
    "@iiko/status": "^2.0.0",
    "@iiko/theming": "^5.0.0",
    "date-fns": "^2.28.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/localize": "^16.2.0",
    "@iiko/eslint-config": "^3.0.0",
    "@iiko/prettier-config": "^2.0.0",
    "@iiko/stylelint-config": "^2.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.1.7",
    "prettier": "^3.0.1",
    "stylelint": "^15.5.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.1.0",
    "typescript": "~5.1.6"
  }
}
